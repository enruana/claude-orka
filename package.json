{
  "name": "@enruana/claude-orka",
  "version": "0.9.2",
  "description": "SDK for orchestrating Claude Code sessions with tmux - Branch management for conversations",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "orka": "./bin/orka.js"
  },
  "scripts": {
    "build": "npm run build:sdk && npm run build:cli && npm run build:web-ui && npm run build:assets",
    "build:assets": "cp src/server/terminal-mobile.html dist/",
    "build:sdk": "tsc",
    "build:cli": "esbuild src/cli/index.ts --bundle --platform=node --format=esm --outfile=dist/cli.js --external:@anthropic-ai/claude-agent-sdk --external:chalk --external:cli-table3 --external:commander --external:execa --external:fs-extra --external:nanoid --external:ora --external:uuid --external:react --external:react-dom --external:lucide-react --external:readline --external:express --external:cors --external:open --external:nodejs-whisper",
    "build:web-ui": "cd src/web-ui && vite build",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "orka": "tsx src/cli/index.ts",
    "cli:link": "npm link",
    "cli:unlink": "npm unlink -g claude-orka",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "claude",
    "claude-code",
    "tmux",
    "orchestrator",
    "sdk",
    "branching",
    "context-management",
    "session-management",
    "ai",
    "cli"
  ],
  "author": "enruana",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enruana/claude-orka.git"
  },
  "bugs": {
    "url": "https://github.com/enruana/claude-orka/issues"
  },
  "homepage": "https://github.com/enruana/claude-orka#readme",
  "files": [
    "dist",
    "bin",
    ".tmux.orka.conf",
    "public/icon.png",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.46",
    "@monaco-editor/react": "^4.7.0",
    "@types/uuid": "^10.0.0",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "execa": "^5.1.1",
    "express": "^5.2.1",
    "fs-extra": "^11.2.0",
    "lucide-react": "^0.554.0",
    "nanoid": "^3.3.7",
    "nodejs-whisper": "^0.1.18",
    "open": "^11.0.0",
    "ora": "^9.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.12.0",
    "remark-gfm": "^4.0.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "esbuild": "^0.27.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^6.4.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
